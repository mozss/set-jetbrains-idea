<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="22" />
    <option name="FONT_FAMILY" value="Droid Sans Mono" />
    <option name="FONT_SCALE" value="2.5" />
    <option name="LINE_SPACING" value="1.0" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>